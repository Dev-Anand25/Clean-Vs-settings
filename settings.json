{
    "vscode_custom_css.imports": [
        " %Path to css file% "
    ],
    "files.autoSave": "onFocusChange",
    "editor.fontFamily": "DM Mono,Consolas, 'Courier New', monospace",
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.iconTheme": "material-icon-theme",
    "explorer.compactFolders": false,
    "editor.cursorBlinking": "phase",
    "workbench.editor.editorActionsLocation": "hidden",

    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "editor.stickyScroll.enabled": false,
    "editor.fontSize": 15,
    "editor.lightbulb.enabled": "off",
    "editor.lineNumbers": "off",
    "workbench.tips.enabled": false,
    "workbench.productIconTheme": "feather-vscode",
    "editor.overviewRulerBorder": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.copyWithSyntaxHighlighting": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.emptySelectionClipboard": false,
    "workbench.editor.enablePreview": false,
    "window.newWindowDimensions": "inherit",
    "editor.multiCursorModifier": "ctrlCmd",
    "files.trimTrailingWhitespace": false,
    "diffEditor.renderSideBySide": false,
    "editor.snippetSuggestions": "top",
    "editor.detectIndentation": false,
    "files.insertFinalNewline": false,
    "files.trimFinalNewlines": false,
    "editor.guides.indentation": false,



    "scm.diffDecorations": "none",
    "editor.hover.delay": 500,
    "editor.hover.enabled": true,
    "editor.matchBrackets": "never",
    "editor.colorDecorators": false,
    "git.decorations.enabled": false,
    "editor.selectionHighlight": false,
    "editor.renderLineHighlight": "none",
    "editor.occurrencesHighlight": "off",
    "problems.decorations.enabled": false,
    "editor.renderControlCharacters": false,
    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.gotoLocation.multipleImplementations": "goto",
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
    "workbench.colorTheme": "Theme Darker",
    "workbench.editor.autoLockGroups": {
        "mainThreadWebview-markdown.preview": true,
        "vscode.audioPreview": true,
        "vscode.videoPreview": true,
        "imagePreview.previewEditor": true,
        "jupyter-notebook": true
    },
    "workbench.editor.sharedViewState": false,
    "workbench.navigationControl.enabled": false,
    "chat.commandCenter.enabled": false,
    "workbench.statusBar.visible": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "window.menuBarVisibility": "compact",
    "workbench.layoutControl.enabled": false,
    "workbench.editor.tabSizing": "fixed",
    "workbench.editor.tabSizingFixedMaxWidth": 120,
}
